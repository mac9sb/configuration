{
  "project_panel": {
    "hide_gitignore": false
  },
  "agent_servers": {
    
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-opus-4.6"
    },
    "play_sound_when_agent_done": true,
    "show_turn_stats": true
  },
  "auto_install_extensions": {
    "colored-zed-icons-theme": true,
    "package-swift-lsp": true,
    "pkl": true,
    "rose-pine-theme": true,
    "swift": true
  },
  "buffer_font_family": "Liga SFMono Nerd Font",
  "buffer_font_size": 11,
  "icon_theme": "Colored Zed Icons Theme Dark",
  "outline_panel": {
    "dock": "right"
  },
  "relative_line_numbers": "enabled",
  "remove_trailing_whitespace_on_save": true,
  "ssh_connections": [
    {
      "host": "dav.internal",
      "projects": [
        {
          "paths": ["/nfs/data/mac/dataloader", "/nfs/appl/museum-in-the-park/"]
        }
      ]
    }
  ],
  "terminal": {
    "button": true,
    "option_as_meta": true
  },
  "theme": {
    "dark": "Rosé Pine",
    "light": "Rosé Pine Dawn",
    "mode": "system"
  },
  "ui_font_features": {
    "ligatures": true
  },
  "ui_font_size": 11,
  "vim_mode": true
}
